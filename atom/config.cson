"*":
  "atom-ternjs": {}
  "atom-typescript": {}
  "autocomplete-python":
    useKite: false
  core:
    telemetryConsent: "limited"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    fontSize: 12
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    softTabs: false
    tabLength: 4
  linter:
    inlineTooltipInterval: 1000
    showErrorPanel: false
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
  "linter-flow": {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
